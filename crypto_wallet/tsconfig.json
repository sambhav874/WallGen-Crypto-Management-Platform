{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "target" : "es6",
    "noEmitOnError": false,
    "checkJs": false,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/SolanaWallet.tsx"],
  "exclude": ["node_modules"],
  "ts-node": {"require": ["tsconfig-paths/register"],
"compilerOptions": {
  "module": "commonjs"
}}
}
